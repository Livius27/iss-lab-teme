<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="hibernate.loc_hbm">

    <class name="model.Loc" table="Locuri">
        <id name="id" column="loc_id" type="int">
            <generator class="increment" />
        </id>
        <property name="numar" column="numar" type="long" />
        <property name="rand" column="rand" type="int" />
        <property name="loja" column="loja" type="int" />
        <property name="pret" column="pret" type="float" />
        <property name="stareLoc" column="stare_loc" type="string" />
    </class>

</hibernate-mapping>
