<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="hibernate.loc_hbm">

    <class name="model.Spectator" table="Spectatori">
        <id name="id" column="spectator_id" type="int">
            <generator class="increment" />
        </id>
        <property name="nume" column="nume" type="string" />
        <property name="prenume" column="prenume" type="string" />
        <property name="email" column="email" type="string" />
    </class>

</hibernate-mapping>
