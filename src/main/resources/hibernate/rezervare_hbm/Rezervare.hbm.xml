<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="hibernate.rezervare_hbm">

    <class name="model.Rezervare" table="Rezervari">
        <id name="id" column="rezervare_id" type="int">
            <generator class="increment" />
        </id>
        <property name="idSpectator" column="spectator_id" type="int" />
        <property name="idLocRezervat" column="loc_rezervat_id" type="int" />
        <property name="titluSpectacol" column="titlu_spectacol" type="string" />
    </class>

</hibernate-mapping>
